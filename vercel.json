{
  "version": 2,
  "builds": [
    {
      "src": "build/web/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/build/web/assets/$1"
    },
    {
      "src": "/flutter_bootstrap.js",
      "dest": "/build/web/flutter_bootstrap.js"
    },
    {
      "src": "/flutter.js",
      "dest": "/build/web/flutter.js"
    },
    {
      "src": "/(.*)",
      "dest": "/build/web/index.html"
    }
  ]
}